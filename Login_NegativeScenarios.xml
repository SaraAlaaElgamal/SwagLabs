<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <parameter name="browser" value=""/>
    <test name="Login With Valid Email">
        <classes>
            <class name="testcases.TC01_Login">
                <methods>
                    <include name="loginWithValidEmail_N"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Login With Valid Password">
        <classes>
            <class name="testcases.TC01_Login">
                <methods>
                    <include name="loginWithValidPassword_N"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Login With Invalid Data">
        <classes>
            <class name="testcases.TC01_Login">
                <methods>
                    <include name="loginWithInvalidData_N"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>